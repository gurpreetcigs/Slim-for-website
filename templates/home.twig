<script type="text/javascript">
    $(document).ready(function () {

        $('#captchamessage').hide();

        $('#submitbtn').click(function (e) {

            var captcha_response = grecaptcha.getResponse();

            if (captcha_response.length == 0) {

                $('#captchamessage').show();

                e.preventDefault();

            }

            else {

                $('#captchamessage').hide();

                $('#contactBootstrapFormFeed').unbind('submit').submit();
            }
        });
        $("#contactmobfeed").keypress(function (e) {
            if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
                $("#errmsg").html("Digits Only").show().fadeOut("slow");
                return false;
            }
        })

        function randomNumber(min, max) {
            return Math.floor(Math.random() * (max - min + 1) + min);
        }

        function generateCaptchaFeedback() {
            $('#captchaOperationFeedback').html([randomNumber(1, 100), '+', randomNumber(1, 200), '='].join(' '));
        }

        generateCaptchaFeedback();

        $('#contactBootstrapFormFeed').formValidation({
            framework: 'bootstrap',
            icon: {
                valid: 'glyphicon glyphicon-ok',
                invalid: 'glyphicon glyphicon-remove',
                validating: 'glyphicon glyphicon-refresh'
            },
            fields: {
                firstName: {
                    row: '.col-xs-4',
                    validators: {
                        notEmpty: {
                            message: 'The first name is required'
                        }
                    }
                },
                lastName: {
                    row: '.col-xs-4',
                    validators: {
                        notEmpty: {
                            message: 'The last name is required'
                        }
                    }
                },
                Mobile: {
                    validators: {
                        notEmpty: {
                            message: 'The phone must be 10 Numbers only'
                        },
                        stringLength: {
                            min: 10,
                            max: 10,
                            message: 'Contact No. is required'
                        },
                        regexp: {
                            regexp: /^[0-9_\.]+$/,
                            message: ''
                        }
                    }
                },
                FullName: {
                    validators: {
                        notEmpty: {
                            message: 'Name  is required'
                        }
                    }
                },
                Email: {
                    validators: {
                        notEmpty: {
                            message: 'The email address is required'
                        },
                        regexp: {
                            regexp: /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/,
                            message: 'The input is not a valid email address'
                        }
                    }
                },
                Subject: {
                    validators: {
                        notEmpty: {
                            message: 'The Subject is required'
                        }
                    }
                },
                Message: {
                    validators: {
                        notEmpty: {
                            message: 'The Message is required'
                        },
                        stringLength: {
                            min: 20,
                            max: 1999,
                            message: 'The Message must consist of atleast 20 &amp; maximum 2000 characters'
                        }
                    }
                },
                password: {
                    validators: {
                        notEmpty: {
                            message: 'The password is required'
                        },
                        different: {
                            field: 'username',
                            message: 'The password cannot be the same as username'
                        }
                    }
                },
                otherSubject: {
                    validators: {
                        notEmpty: {
                            message: 'Enter subject is required'
                        }
                    }
                },
                agree: {
                    validators: {
                        notEmpty: {
                            message: 'You must agree with the terms and conditions'
                        }
                    }
                },
                captcha: {
                    validators: {
                        callback: {
                            message: 'Wrong answer',
                            callback: function (value, validator, $field) {
                                var items = $('#captchaOperationFeedback').html().split(' '),
                                sum = parseInt(items[0]) + parseInt(items[2]);
                                return value == sum;
                            }
                        }
                    }
                }
            }
        }).on('err.form.fv', function (e) {
            generateCaptchaFeedback();
        });
    });

    function ShowOthersSubject() {
        var subselect = document.getElementById("ddlsubjects");
        var disabledbox = document.getElementById("otherfield");
        var selectedText = subselect.options[subselect.selectedIndex].text;
        if (selectedText == "Other") {
            $('#otherfield').removeClass('hidden');
        }
        else {
            $('#otherfield').addClass('hidden');
        }
    }

    function feedbackclick() {
        var isValidForm = $('#contactBootstrapFormFeed').data('formValidation').isValid();
        if (isValidForm) {
            var formdata = $("#contactBootstrapFormFeed");
            $.ajax({
                type: "POST",
                url: "/Home/PostFeedback", // the URL of the controller action method
                data: formdata.serialize(), // optional data
                dataType: 'json',// optional data
                success: function (req) {
                    if (req.responseText != "") {
                        alert('thanks query submitted');
                    }
                    else {
                        alert('an error occured while processing your request');
                    }
                },
                error: function (req, status, error) {
                    alert('thanks query submitted');
                }
            });
        }
    }
