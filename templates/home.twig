            if (term > 50)
                $("#Term").val(50);

            if (term <= 0 || isNaN(term)) {
                $("#Term").val(2);
                term = $("#Term").val();
                $("#DelayTerm").val(term - 1);
            }

            if (delayterm >= term || isNaN(term)) {

                $("#DelayTerm").val(term - 1);
            }
            reload_page();
        }

        function Rupee(temp) {
            var x = temp;
            x = x.toString();
            var lastThree = x.substring(x.length - 3);
            var otherNumbers = x.substring(0, x.length - 3);
            if (otherNumbers != '')
                lastThree = ',' + lastThree;
            var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree;
            return res;
